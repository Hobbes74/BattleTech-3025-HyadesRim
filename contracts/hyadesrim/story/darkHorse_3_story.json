            },
            "name" : "Inter_VictoriaGreeting",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "So you're the upstart failed heir who thinks this tournament will change your luck, heh?",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
