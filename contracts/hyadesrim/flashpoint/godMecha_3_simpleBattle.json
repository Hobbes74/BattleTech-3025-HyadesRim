            "name" : "Dialogue_MissionStart",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "The mist will come and the fog of battle will bind us all!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_AutocracyOfLehLeaderDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Yang, most of our BattleMech weapons seem to be non-operational...?",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "And in the Mar'tu the wanderers shall be revealed!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_AutocracyOfLehLeaderDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "These are not <i>our</i> 'Mechs, remember?",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_YangDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "d9a28c08-abdb-42bb-a140-ad3927d1f8cf",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "For they shall require no weapons to win other than melee combat!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_AutocracyOfLehLeaderDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Contenders, only short range combat is allowed!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_AutocracyOfLehLeaderDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                },
                {
                    "words" : "Now they tell us?!? We need to have a conversation when this is over!",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_DariusDefault",
