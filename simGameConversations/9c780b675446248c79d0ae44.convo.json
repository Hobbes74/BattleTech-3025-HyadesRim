    {
      "idRef": {
        "id": "9ca67b97eb87eaba310ede0a"
      },
      "index": 9,
      "text": "Nevermind that the [[DM.BaseDescriptionDefs[LoreAresConventions],Ares Conventions]] ban the use of such weapons, no one would ever think of using them again nowadays with the destruction that they would bring.\n\nI am surprised that your family went along with this project, Alexander.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 10,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9cedd06198a70aa6df6470ea"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
      "override_speaker": null,
      "speaker_override_id": "59b98a97623035ac140056c6",
