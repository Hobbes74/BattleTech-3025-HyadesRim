    {
      "idRef": {
        "id": "9cf42cd0fbbd1782f5231036"
      },
      "index": 3,
      "text": "No Commander, there is something vital that you need to retrieve from the one of Guldra's moons and bring back to Coromodir before the Harbingers arrive.\n\nThe fate of the invasion could depend on success of this mission.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 4,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c864e7bd19c3c64f32afc97"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
