    {
      "idRef": {
        "id": "9c0d69c058d8c80cb08b27a5"
      },
      "index": 4,
      "text": "You are exaggerating our customs and I invite you all to see for yourselves. Once the battle is over, the victors are required to perform the celebratory parade across Leh, for the faithful to see and worship God Mecha, before receiving their final victory prize.\n\nThen you are free to leave or remain, we will not impose our ways upon you or your MechWarriors. Do you have any additional questions?",
      "branches": [
        {
          "responseText": "No, thank you Sister Rebecca. That will be all for now.",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 5,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c715e64f0630321bfd4df31"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
