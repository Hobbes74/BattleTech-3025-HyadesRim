    {
      "idRef": {
        "id": "9c103b2735eb032b51cd01fc"
      },
      "index": 9,
      "text": "Why do you think they would ally with us against the Autocracy and the Arauts?",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 10,
          "hideIfUnavailable": true,
          "onlyOnce": false,
