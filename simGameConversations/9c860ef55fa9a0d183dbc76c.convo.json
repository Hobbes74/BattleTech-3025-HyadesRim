    {
      "idRef": {
        "id": "9cc716879d01af4a6bbb02b0"
      },
      "index": 16,
      "text": "I know what you're talking about and we get the point you're making.",
      "branches": [
        {
          "responseText": "What do you want from us?",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 17,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9cd15f640b93a940a51a67c8"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
