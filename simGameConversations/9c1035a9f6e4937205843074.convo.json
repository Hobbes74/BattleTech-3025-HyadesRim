      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": {
        "id": "TortugaPirateLeaderDefault"
      },
      "sourceWithTagInScene": "",
      "override_speaker": null,
      "speaker_override_id": "",
      "actions": null,
      "comment": ""
