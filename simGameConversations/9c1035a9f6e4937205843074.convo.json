      "idRef": {
        "id": "9cea107c080cb3f62b649a00"
      },
      "index": 10,
      "text": "And bang, she's gone for good, probably already jumped off system.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 11,
          "hideIfUnavailable": true,
