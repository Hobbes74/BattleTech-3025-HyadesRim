    {
      "idRef": {
        "id": "9c9be8eae00aef68c9c1e4da"
      },
      "index": 4,
      "text": "{COMMANDER.FirstName}.\n\nI assume your talk with Precentor Cattus went well?",
      "branches": [
        {
          "responseText": "Very well, indeed. He was most pleased that we were able to ransom him from your pirate group.",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 5,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c5254553f299207dddbe5a9"
          },
