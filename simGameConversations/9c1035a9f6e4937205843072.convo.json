      "speaker_override_id": "",
      "actions": null,
      "comment": ""
    },
    {
      "idRef": {
        "id": "9c0248757020eb5f5596c728"
      },
      "index": 2,
      "text": "One of our outposts on this planet was taken by [[DM.Factions[faction_HarbingersOfDoom],Harbinger]] raiders in possession of BattleMech forces. We require one of your medium lances to escort our security forces while we assault it and engage the pirate 'Mechs.\n\nWe also require one of your technical teams for relief efforts and to restore life support since some of the structures might still contain survivors.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 3,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c1636d5715cb931ae45d5d2"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": {
        "id": "ComStarPrecentorCattusDefault"
      },
      "sourceWithTagInScene": "",
