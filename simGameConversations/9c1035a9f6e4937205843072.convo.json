    {
      "idRef": {
        "id": "9cae68705d2aeb2ca0bfdd9d"
      },
      "index": 1,
      "text": "Are you? Really? We will see about how dedicated you are to [[DM.BaseDescriptionDefs[LoreJeromeBlake],Blake]]'s path soon.\n\nWe run a number of humanitarian efforts in this system that are being harassed by one of the pirate gangs operating in this area, the [[DM.BaseDescriptionDefs[LoreHarbingersOfDoom],Harbingers of Doom]].",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 2,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9cfa5ca9b4ecdad0fec9f9e9"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": {
        "id": "ComStarPrecentorCattusDefault"
      },
      "sourceWithTagInScene": "",
