    {
      "idRef": {
        "id": "9cae111062122c4806cadbab"
      },
      "index": 5,
      "text": "Yeah, no one here wants to get into your Blessed Order's black list. \n\nSorry for any misunderstanding.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 6,
          "hideIfUnavailable": true,
          "onlyOnce": false,
