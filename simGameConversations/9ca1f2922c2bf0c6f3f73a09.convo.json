    {
      "idRef": {
        "id": "9c6f8e5d5c3fbe9ca73db80b"
      },
      "index": 9,
      "text": "Paint and serial numbers aren't exactly solid evidence, Yang.\n\nHave you discussed this with {COMMANDER.FirstName}?",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 10,
          "hideIfUnavailable": true,
          "onlyOnce": false,
