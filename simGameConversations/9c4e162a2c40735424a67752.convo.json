      "speaker_override_id": "59b98a97623035ac140056c6",
      "actions": null,
      "comment": ""
    },
    {
      "idRef": {
        "id": "9c75c8b31569e5dde605ed2a"
      },
      "index": 12,
      "text": "I'm not suggesting anything. I'm telling you that these invaders have BattleMech technologies either lost or never seen before. \n\nIf you think I'm lying, come up with your own conclusions next time you face them. And you soon will.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 13,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c5549148a5d58b8acfbd04f"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": {
