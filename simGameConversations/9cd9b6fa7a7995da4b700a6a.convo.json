          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
      "override_speaker": null,
      "speaker_override_id": "59b98a97623035ac140056c6",
      "actions": null,
      "comment": ""
