    {
      "idRef": {
        "id": "9ce8a7be0d7bfc33abf65790"
      },
      "index": 5,
      "text": "Excuse me Lady Ana Maria, does that warehouse contain anything that can endanger the operation?\n\nThis supplementary contract isn't unusual but we would need to know exactly what we're dealing here with, specially if it involves any kind of weaponry or any potential hazard for DropShip operations.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 6,
          "hideIfUnavailable": false,
          "onlyOnce": false,
          "idRef": {
            "id": "9ce52ce964cc7b35f69b4cf9"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
      "override_speaker": null,
      "speaker_override_id": "598cd3a06230355c18000067",
      "actions": null,
      "comment": ""
