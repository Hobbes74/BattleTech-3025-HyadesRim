    {
      "idRef": {
        "id": "9cf49567f2e9c0ef685514df"
      },
      "index": 4,
      "text": "That's not entirely required for your company to know, Doctor. \n\nBefore you deploy your company to defend Majesty Metals HQ it will be required for you to raid one of TTI's facilities on another continent. Once you secure the warehouse we'll take over and your forces will redeploy immediately to support the evacuation effort on the planetary capital.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 5,
          "hideIfUnavailable": false,
          "onlyOnce": false,
          "idRef": {
            "id": "9cb4a68a83cff59ceba82baa"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
