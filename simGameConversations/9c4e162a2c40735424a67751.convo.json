    {
      "idRef": {
        "id": "9cd078f98523e6e630020101"
      },
      "index": 2,
      "text": "This is Captain Laura Murray of the [[DM.BaseDescriptionDefs[LoreTortugaPirates],Satan's Kittens]].\n\nTo Commander {COMMANDER.LastName} of the {COMPANY.CompanyName}, you're about to finally be blown to pieces by three DropShips, unless you follow their instructions to accompany them to the moon.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 3,
          "hideIfUnavailable": true,
          "onlyOnce": false,
