    {
      "idRef": {
        "id": "9ca2a0556fe2e4baa73ac91c"
      },
      "index": 12,
      "text": "They made their orbital insertion on the other side of the moon while we dropped you in the <i>Hysteria</i> so you should be safe for now.\n\nBut the <i>Argo</i> is almost defenseless every time we deploy her, so we're risking one of those landed DropShips coming after us in orbit.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 13,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c269e2bfc5faeca14ac9098"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
