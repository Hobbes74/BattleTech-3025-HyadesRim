    {
      "idRef": {
        "id": "9c6944efdf88470a71216def"
      },
      "index": 9,
      "text": "Whatever it is, you all need to hurry.\n\nA group of unknown DropShips has landed several miles from your position and their lead elements will be entering the valley where the entrance is located soon.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 10,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
