      "comment": ""
    },
    {
      "idRef": {
        "id": "5a6a735f623035a0200000ca"
      },
      "index": 8,
      "text": "What kind of stories, exactly?",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 9,
          "hideIfUnavailable": false,
          "onlyOnce": false,
