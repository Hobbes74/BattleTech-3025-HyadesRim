          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
      "override_speaker": null,
      "speaker_override_id": "59baac916230356416000354",
      "actions": null,
      "comment": ""
