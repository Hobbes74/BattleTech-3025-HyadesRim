    {
      "idRef": {
        "id": "9c7ea0b59d90d8b53e222fc1"
      },
      "index": 6,
      "text": "As for the first question, the [[DM.BaseDescriptionDefs[LoreDavionAFFS],AFFS]] is currently conducting a covert supply operation using Lyreton as one of its routes.\n\n[[DM.Factions[faction_Davion],Davion]] JumpShips have been using unmarked systems to bypass the Taurian worlds and this is their hub before their cargo is shipped out to other systems.",
      "branches": [
        {
          "responseText": "Lord Kurita, what kind of cargo?",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 7,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c0678e64ddf377a23041917"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": {
