    {
      "idRef": {
        "id": "9c1175e795fbc911853a96b5"
      },
      "index": 2,
      "text": "Commander {COMMANDER.LastName} of the {COMPANY.CompanyName}, my pleasure to see you and I hope this message finds you well in these troubled times. \n\nI am acting both as employer and as a liaison for the [[DM.BaseDescriptionDefs[LoreAlphardTradingCompany],Alphard Trading Company]], who issued the original extraction contract against the [[DM.BaseDescriptionDefs[LoreTortugaPirates],Satan's Kittens]].",
      "branches": [
        {
          "responseText": "Yes... it is also our pleasure to see you again, Lord Singh. How may we assist you?",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 3,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c11949c7e4ffdd6f50c5006"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
