      "speaker_override_id": "598cd3a06230355c18000067",
      "actions": null,
      "comment": ""
    },
    {
      "idRef": {
        "id": "9c16e2e137db90ef366599dc"
      },
      "index": 13,
      "text": "The moment Murray steps into the battlefield we can expect her to recognize us after the events at The Twins. \n\nParrot FrankenMech or not, that should be a interesting moment indeed!",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 14,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c62291f40ada96eee10526f"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
      "override_speaker": null,
      "speaker_override_id": "598cd3f36230355c1800006a",
