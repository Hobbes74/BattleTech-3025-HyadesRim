    {
      "idRef": {
        "id": "9cd8be2450ea5ab839f12cf1"
      },
      "index": 7,
      "text": "Let's just say that the Magestrix also wants to send you and your company a message, Commander.",
      "branches": [
        {
          "responseText": "Understood.",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 8,
          "hideIfUnavailable": true,
          "onlyOnce": false,
