    {
      "idRef": {
        "id": "9ccac79c517d7f623672bf0a"
      },
      "index": 6,
      "text": "Yes Commander, the same one senator that offered you the contract to raid our vaccine supplies at Luxen.\n\nThe Magestrix wants you to send a very special message to Imperator O'Reilly to serve as a lesson by assassinating him.",
      "branches": [
        {
          "responseText": "You could have used your operatives or someone else, why us?",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 7,
          "hideIfUnavailable": true,
          "onlyOnce": false,
          "idRef": {
            "id": "9c13da4d99c647867387cad0"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
