    {
      "idRef": {
        "id": "9c4dbe07f924b429e90a4c4a"
      },
      "index": 10,
      "text": "Yang, you're being too paranoid. Why would someone make such an elaborate trap just in order to get Kamea?",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 11,
