    },
    {
      "idRef": {
        "id": "9c673d72d7f08ba6d7d7189b"
      },
      "index": 9,
      "text": "Could human-cat hybrids have been that important for them to hide this so deep?",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 10,
          "hideIfUnavailable": true,
