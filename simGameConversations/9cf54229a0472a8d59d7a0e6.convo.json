    {
      "idRef": {
        "id": "9cd7ac65d59ca2050249365e"
      },
      "index": 41,
      "text": "Commander, a retainer contract is exactly what we were looking for.",
      "branches": [
        {
          "responseText": "Interesting. We will still be able to take independent missions and travel?",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 42,
