    {
      "idRef": {
        "id": "9c300cc54c952297dc6a0543"
      },
      "index": 1,
      "text": "Please Commander {COMMANDER.LastName}, just call me Marcus, we're too far from the Senate at [[DM.BaseDescriptionDefs[LoreAlphard],Alphard]] for such formalities.\n\nI may be a patrician but I recognize the value of the plebs and their simpler ways.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 2,
          "hideIfUnavailable": true,
