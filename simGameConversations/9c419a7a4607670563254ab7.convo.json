    {
      "idRef": {
        "id": "9c40ed13eae0c8e023a8efea"
      },
      "index": 7,
      "text": "A risk avoidance necessary for the safety of everyone, I'm not bringing back to the Hegemony a new pandemic.\n\nMy duty to the Hegemony is satisfied and I declare your contractual obligations have been fulfilled.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
